{
  "compilerOptions": {
    "outDir": "theme/static",
    "target": "esnext",
    "module": "esNext",
    "lib": ["es2015", "dom"],
    "include": [
      "./src/"
    ],
    "allowJs": true,
    "noEmit": true,
    "strict": false,
    "moduleResolution": "node",
    "baseUrl": "./assets/",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true
  }
}
